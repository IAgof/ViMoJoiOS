update_fastlane

fastlane_version "2.31.0"

default_platform :ios

platform :ios do
  lane :certificates do
      match(type: 'development',app_identifier: ["com.videona.vimojo", "com.vimojo.thomsonfoundation", "com.vimojo.m4n", "com.vimojo.elconfidencial", "com.vimojo.RTVE"], readonly: false)
      match(type: 'adhoc', app_identifier: ["com.videona.vimojo", "com.vimojo.thomsonfoundation", "com.vimojo.m4n", "com.vimojo.elconfidencial", "com.vimojo.RTVE"], readonly: false)
  end
  # Fastfile actions accept additional configuration, but
  # don't worry, fastlane will prompt you for required
  # info which you can add here later
  lane :beta do
    match(type: 'adhoc', app_identifier: ["com.videona.vimojo"])
    # build your iOS app
    build_app(
      scheme: "vimojo",
      export_method: "ad-hoc"
    )

    # upload to Beta by Crashlytics
    crashlytics(
      # keys for organization: DEPRECATED-VideonaIOS
